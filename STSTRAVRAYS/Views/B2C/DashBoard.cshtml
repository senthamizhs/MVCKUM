<!--  Add Traveller -->
<div class="modal fade" id="modal-addtravel" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="row">
        <div class="modalshow bg-white">
            <div class="modal-tit float-left w-100 col-sm-12 col-xs-12 bg-clr-cmmntheme pt-10 pb-10 mb-15">
                <h5 class="white-text float-left mg-btm-0 text-transform-c font-16 mt-0" id="changeName">Edit Traveller</h5>
                <i class="las la-times white-text float-right cursor-point fnt-siz-20" onclick="clearpopup()" data-dismiss="modal"></i>
            </div>
            <div class="modalbody dash-area">
                <div class="row row0">
                    <div class="col-sm-12 col-xs-12 pad-0-res">
                        <div class="col-sm-4 col-xs-6 mb-15 pos-rel">
                            <label class="widget_data float-left txt-al-lft font-12 fnt-wt-600-im mb-2">Pax Type</label>
                            <select id="ddl_paxtype" class="font-14 widget_label form-control" onchange="changetitle()">
                                <option value="Adult" selected=""> Adult</option>
                                <option value="Child">Child</option>
                                <option value="Infant">Infant</option>
                            </select>
                        </div>
                        <div class="col-sm-4 col-xs-6 mb-15 pos-rel">
                            <label class="widget_data float-left txt-al-lft font-12 fnt-wt-600-im mb-2">Title</label>
                            <select id="ddl_title" class="font-14 widget_label form-control">
                                <option value="">Select</option>
                                <option value="Mstr.">Mstr.</option>
                                <option value="Miss.">Miss.</option>
                            </select>
                        </div>
                        <div class="col-sm-4 col-xs-6 mg-btm-15 pos-rel">
                            <label class="widget_data float-left txt-al-lft font-12 fnt-wt-600-im mb-2">First Name</label>
                            <form autocomplete="off" novalidate="novalidate">
                                <input type="text" id="txtfname" required="" class="font-14 widget_label form-control" placeholder="Enter First Name" oninput="uppercase()" onkeypress="nameval(event)" maxlength="25">
                            </form>
                        </div>
                        <div class="col-sm-4 col-xs-6 mg-btm-15 pos-rel">
                            <label class="widget_data float-left txt-al-lft font-12 fnt-wt-600-im mb-2">Last Name</label>
                            <form autocomplete="off" novalidate="novalidate">
                                <input type="text" id="txtlname" required="" class="font-14 widget_label form-control" placeholder="Enter Last Name" oninput="uppercase()" onkeypress="nameval(event)" maxlength="15">
                            </form>
                        </div>
                        <div class="col-sm-4 col-xs-6 mg-btm-15 pos-rel">
                            <label class="widget_data float-left txt-al-lft font-12 fnt-wt-600-im mb-2">Date of Birth</label>
                            <input type="text" id="txtdob" required="" class="font-14 widget_label form-control hasDatepicker" placeholder="DD/MM/YYYY" readonly="">
                        </div>
                        <div class="col-sm-4 col-xs-6 mg-btm-15 pos-rel">
                            <label class="widget_data float-left txt-al-lft font-12 fnt-wt-600-im mb-2">Nationality</label>
                            <select id="ddl_nationality" class="font-14 widget_label form-control">
                            </select>
                        </div>
                        <div class="col-sm-4 col-xs-6 mg-btm-15 pos-rel">
                            <label class="widget_data float-left txt-al-lft font-12 fnt-wt-600-im mb-2">Passport Number</label>
                            <form autocomplete="off" novalidate="novalidate">
                                <input type="text" id="txtpassport" maxlength="10" required="" class="font-14 widget_label form-control" onkeypress="passportvalidate(event)" oninput="uppercase()" placeholder="Enter Passport Number">
                            </form>
                        </div>
                        <div class="col-sm-4 col-xs-6 mg-btm-15 pos-rel">
                            <label class="widget_data float-left txt-al-lft font-12 fnt-wt-600-im mb-2">Passport Expiry Date</label>
                            <form autocomplete="off" novalidate="novalidate">
                                <input type="text" id="txtpasportexpiredate" readonly="" required="" class="font-14 widget_label form-control hasDatepicker" placeholder="DD/MM/YYYY">
                            </form>
                        </div>
