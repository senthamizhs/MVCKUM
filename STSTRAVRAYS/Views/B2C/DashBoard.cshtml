<script>
    var strFlightImgVersion = '@strFlightImgVersion';
    var strAirlineLogoURL = '@strAirlineLogoURL';
    var strLoaderURL = '@strLoaderURL';
    var strMobileDevice = '@strMobileDevice'
    var strNoofMonths = strMobileDevice == "Y" ? 2 : 1;
    var strFetchMyBookingsURL = '@Url.Action("FetchMyBooking","B2C")';
    var strFetchMyTravellersURL = '@Url.Action("FetchMyTravellers","B2C")';
    var strUpdateMyProfileURL = '@Url.Action("UpdateMyProfile", "B2C")';
    var PrintTicketRedirect = '@Url.Action("PrintTickets", "AfterBooking")';
    var Mailsendingurl = '@Url.Action("btnEmail_pdforhtml_Click", "AfterBooking")';
    var strFetchMyViewPNRURL = '@Url.Action("FetchMyViewPNR", "B2C")';
    var strUpdatePasswordURL = '@Url.Action("UpdateMyPassword", "B2C")';
    var strCreditShellRedirectURL = '@Url.Action("OtherProduct", "OtherProduct")';
    var CountryxmlURL = '@Url.Content("~/XML/Country.xml")';
    var ConvertPDF_URL = "@Url.Action("pdfTktconverter", "AfterBooking")"
    var FetchPrintTicketURL = "@Url.Action("GetTicketCopy", "AfterBooking")"
    var strDefaultProfile = '@strDefaultProfile';
    var strUserProfileImg = '@strUserProfile';
    var strUserTitle = '@strUserTitle';
    var strUserFname = '@strUserFName';
    var strUserLname = '@strUserLName';
    var strUserMobileNO = '@strUserMobileNo';
    var strUserEmailID = '@strUserEmailID';
    var strUserAddress = '@strUserAddress';
    var strUserCity = '@strUserCity';
    var strUserCountry = '@strUserCountry';
    var strUserPassportNo = '@strUserPassportNO';
    var strUserDOB = '@strUserDOB';
    var strUserPassEXP = '@strUserPass_EXP';
    var strUserPassCountry = '@strUserISSU_Country';
    var strUserPincode = '@strUserPincode';
    var Today = GetTodayDate();
    var strDate = Today.split('/')
    var mindate = CustomDateFormat(strDate, 0, 6, 0, '-');
    var maxdate = CustomDateFormat(strDate, 1, 0, 0, '+');
    $('body').data('OLD_PWD', '@strUserPassword');
    var Flag = '';
    if ('@strTerminalType' == 'M') {
        Flag = '@ViewBag.Flag'
    }
    else {
        Flag = sessionStorage.getItem("DashBoardKey") != null ? sessionStorage.getItem("DashBoardKey") : "";
    }
    var strQueryString = Flag;
    if (strQueryString == "" || strQueryString == "D") {
        FetchupComingTrips();
    }
    else {
        if (strQueryString == "B") {
            ChangeTabs('MyBookings', 'B')
        }
        else if (strQueryString == "U") {
            ChangeTabs('MyUpComing', 'U')
        }
        else if (strQueryString == "C") {
            ChangeTabs('MyCancellation', 'C')
        }
        else if (strQueryString == "S") {
            ChangeTabs('Support')
        }
        else if (strQueryString = 'P') {
            ChangeTabs('MyProfile')
        }
    }

    $(document).ready(function () {
        $("#txtFromDate_Book").val(Today);
        $("#txtFromDate_Book").datepicker({
            minDate: mindate, maxDate: Today, dateFormat: "dd/mm/yy", numberOfMonths: strNoofMonths,
        });
        $("#txtToDate_Book").datepicker({
            minDate: $("#txtFromDate_Book").val(), maxDate: maxdate, dateFormat: "dd/mm/yy", numberOfMonths: strNoofMonths,
        });
        $("#txtToDate_Book").val(Today);
        $("#txtFromDate_Book").on('change', function () {
            $("#txtToDate_Book").datepicker("destroy");
            $("#txtToDate_Book").datepicker({
                minDate: $("#txtFromDate_Book").val(), maxDate: maxdate, dateFormat: "dd/mm/yy", numberOfMonths: strNoofMonths,
            });
            $("#txtToDate_Book").val($("#txtFromDate_Book").val());
        });
        LoadCountry();
        ResetProfile();
    });
</script>