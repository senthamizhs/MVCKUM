@using System.Configuration;
@{
    string strTheme = ConfigurationManager.AppSettings["apptheme"].ToString();
    string strAirlineLogoURL = ConfigurationManager.AppSettings["FlightUrl"].ToString();
    string strFlightImgVersion = ConfigurationManager.AppSettings["flightimageversion"].ToString();
    string strFileversion = ConfigurationManager.AppSettings["UpdateVersion"].ToString();
    string strProductType = ConfigurationManager.AppSettings["Producttype"] != null ? ConfigurationManager.AppSettings["Producttype"].ToString() : "";
    string strLoaderURL = ConfigurationManager.AppSettings["LogoUrl"] + strProductType + "/" + "loader.gif?" + strFileversion;
    string strUserMobileNo = Session["USERMOBILENO"] != null && Session["USERMOBILENO"].ToString() != "" ? Session["USERMOBILENO"].ToString() : "";
    string strUserEmailID = Session["USERNAME"] != null && Session["USERNAME"].ToString() != "" ? Session["USERNAME"].ToString() : "";
    string strUserFName = Session["USERFNAME"] != null && Session["USERFNAME"].ToString() != "" ? Session["USERFNAME"].ToString() : "";
    string strUserLName = Session["USERLNAME"] != null && Session["USERLNAME"].ToString() != "" ? Session["USERLNAME"].ToString() : "";
    string strUserTitle = Session["USERTITLE"] != null && Session["USERTITLE"].ToString() != "" ? Session["USERTITLE"].ToString() : "";
    string strUserAddress = Session["USERADDRESS"] != null && Session["USERADDRESS"].ToString() != "" ? Session["USERADDRESS"].ToString() : "";
    string strUserCity = Session["USERCITY"] != null && Session["USERCITY"].ToString() != "" ? Session["USERCITY"].ToString() : "";
    string strUserCountry = Session["USERCOUNTRY"] != null && Session["USERCOUNTRY"].ToString() != "" ? Session["USERCOUNTRY"].ToString() : "";
    string strUserPassportNO = Session["USERPASSPORTNO"] != null && Session["USERPASSPORTNO"].ToString() != "" ? Session["USERPASSPORTNO"].ToString() : "";
    string strUserDOB = Session["USEROB"] != null && Session["USEROB"].ToString() != "" ? Session["USEROB"].ToString() : "";
    string strUserPass_EXP = Session["USERPASSPORT_EXP"] != null && Session["USERPASSPORT_EXP"].ToString() != "" ? Session["USERPASSPORT_EXP"].ToString() : "";
    string strUserISSU_Country = Session["USERPASS_ISSU_COUNTRY"] != null && Session["USERPASS_ISSU_COUNTRY"].ToString() != "" ? Session["USERPASS_ISSU_COUNTRY"].ToString() : "";
    string strUserPincode = Session["USERPINCODE"] != null && Session["USERPINCODE"].ToString() != "" ? Session["USERPINCODE"].ToString() : "";
    string strLogoUrl = ConfigurationManager.AppSettings["LogoUrl"].ToString();
    string strDefaultProfile = (strLogoUrl + strProductType + "/DASHBOARD/profile_img.png");
    string strUserProfile = Session["USERPROFILEPIC"] != null && Session["USERPROFILEPIC"].ToString() != "" ? Session["USERPROFILEPIC"].ToString() : strDefaultProfile;
    string strUserPassword = Session["PWD"] != null && Session["PWD"].ToString() != "" ? Session["PWD"].ToString() : "";
    string strTerminalType = Session["TERMINALTYPE"] != null && Session["TERMINALTYPE"].ToString() != "" ? Session["TERMINALTYPE"].ToString() : ConfigurationManager.AppSettings["TerminalType"].ToString().ToUpper();
    string strMobileDevice = Request.Browser.IsMobileDevice ? "Y" : "N";
}
