<!-- change password -->
<div class="modal fade pad-0-res" id="change-password" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="row">
        <div class="modalshow bg-white">
            <div class="modal-tit float-left w-100 col-sm-12 col-xs-12 bg-clr-cmmntheme pt-10 pb-10 mb-0">
                <h5 class="white-text float-left mg-btm-0 text-transform-c font-14 fnt-wt-600 mt-0 pl-10" id="changeName">Change Password</h5>
                <i class="las la-times white-text float-right cursor-point fnt-siz-20 pr-10" data-dismiss="modal"></i>
            </div>
            <div class="modalbody dash-area">
                <div class="col-sm-5 col-xs-12 col-sm-offset-4 col-xs-offset-0 bg-white mg-tp-30 mg-tp-0-res">
                    <div class="row">
                        <div class="col-sm-12 col-xs-12 pt-10 pb-10 pass_sec">
                            <div class="w-100 txt-center pass_img p-10 brdr-radius-2">
                                <img src="~/Images/@strProductType/DASHBOARD/lock.png" />
                            </div>
                            <div class="col-sm-12 col-xs-12 mg-btm-20">
                                <h4 class="flt-lft w-100 txt-center font-18">Change Password</h4>
                            </div>
                            <form>
                                <div class="col-xs-12 change_new_old_confirm_pass mg-btm-30">
                                    <div class="dv-anim input-effect m-0">
                                        <input id="Old_pwdd" maxlength="20" class="txt-anim w-100 clsPassword" type="password" autocomplete="new-password">
                                        <label class="font-13">Old Password<span style="color: red;">*</span></label>
                                    </div>
                                </div>
                                <div class="col-xs-12 change_new_old_confirm_pass mg-btm-30">
                                    <div class="dv-anim input-effect m-0">
                                        <input id="New_pwdd" maxlength="20" class="txt-anim w-100 clsPassword" type="password" autocomplete="new-password">
                                        <label class="font-13">New Password<span style="color: red;">*</span></label>
                                    </div>
                                </div>
                                <div class="col-xs-12 change_new_old_confirm_pass mg-btm-30">
                                    <div class="dv-anim input-effect m-0">
                                        <input id="Cnf_pwdd" maxlength="20" class="txt-anim w-100 clsPassword" type="password" autocomplete="new-password">
                                        <label class="font-13">Confirm Password<span style="color: red;">*</span></label>
                                    </div>
                                </div>
                            </form>

                            <div class="col-xs-12">
                                <div class="cntr txt-cntr m-b-10">
                                    <input style="display: none;" id="serv_checkstandard" class="cb" type="checkbox" value="0" onchange="togglePassword();">
                                    <label class="cbx" for="serv_checkstandard"></label>
                                    <label class="lbl font-13" for="serv_checkstandard">Show Password</label>
                                </div>
                            </div>
                            <div class="col-xs-12 col-md-12 col-sm-12 col-lg-12" style="margin-bottom: 15px">
                                <div class="col-xs-6 col-md-6 col-sm-6 col-lg-3 col-lg-offset-3">
                                    <input type="button" id="change_pwd" class="action-button shadow animate color" style="width: 100%" value="Save" onclick="UpdatePassword()">
                                </div>
                                <div class="col-xs-6 col-md-6 col-sm-6 col-lg-3">
                                    <input type="button" id="clear" class="action-button action-buttonB shadow animate  colorB1" style="width: 100%" value="Clear" onclick="clearPassword()" data-dismiss="modal">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
