            function Individual() {
                $("#btnAddVisa").hide();
                $("#btnRemoveVisa").hide();
                var strPassengerCount = $("#hdnPassengerCount").val();
                addGrpPasenger(strPassengerCount);
                $("#OverallDetails_" + strPassengerCount).css("display", "block");
            }
            function GroupBooking() {
                $("#btnAddVisa").show();
                $("#btnRemoveVisa").show();
                var strGrpCnt = $("#hdnGrpPassengerCount").val();
                addGrpPasenger(strGrpCnt);
            }
            function addVisa() {
                if (($("#indivdual").is(":checked")) == true) {
                    var strVisaCnt = $("#hdnPassengerCount").val();
                    if (strVisaCnt <= 5) {
                        var strcnt = parseInt(strVisaCnt) + 1;
                        $("#hdnPassengerCount").val(strcnt);
                        addPasenger(strcnt);
                    }
                }
                else {
                    var strVisaCnt = $("#hdnGrpPassengerCount").val();
                    if (strVisaCnt <= 5) {
                        var strcnt = parseInt(strVisaCnt) + 1;
                        $("#hdnGrpPassengerCount").val(strcnt);
                        addGrpPasenger(strcnt);
                    }

                }
            }
            function removeVisa() {
                if (($("#indivdual").is(":checked")) == true) {
                    var strVisaCnt = $("#hdnPassengerCount").val();
                    if (strVisaCnt > 1) {
                        document.getElementById("OverallDetails_" + strVisaCnt).remove();
                        var strcnt = parseInt(strVisaCnt) - 1;
                        $("#hdnPassengerCount").val(strcnt);
                    }
