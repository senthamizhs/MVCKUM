                if (input.files && input.files[0]) {
                    var reader = new FileReader();
                    reader.onload = function (e) {
                        $("#" + hidearg).hide();
                        $("#" + apparg).attr('src', e.target.result);
                        $("#" + showarg).show();
                        $("#" + hidearg + '.image-title').html(input.files[0].name);
