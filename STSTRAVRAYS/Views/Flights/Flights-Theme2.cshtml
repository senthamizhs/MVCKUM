    function fetchpaymentmode(AgentId) {
        $.ajax({
            type: "POST", 		//GET or POST or PUT or DELETE verb
            url: '@Url.Action("BalanceCheck", "Home")',// Location of the service
            data: '{AgentId:"' + AgentId + '"}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (json) {//On Successful service call
                if (json.Result[2] != null && json.Result[2] != "") {
                    var Air_Thread = $("#hdn_Airthread").val();
                    var jsonarray = JSON.parse(json.Result[2]);
                    var Thread = jsonarray[0]["Thread"] + (Air_Thread != "" ? "|" + Air_Thread : "");
                    var FscThread = jsonarray[0]["FscThread"];
                    var LccThread = jsonarray[0]["LccThread"];
                    var MultiThread = jsonarray[0]["MultiThread"];
                    var SFCThread = jsonarray[0]["SFC"];
                    var NewThread = jsonarray[0]["NewThread"];

                    for (property in jsonarray[0]) {
                        if (property == "ThreadD") {
                            var D_threadkey = jsonarray[0]["ThreadD"];
                        }
                        if (property == "FscThreadD") {
                            var D_FscThread = jsonarray[0]["FscThreadD"];
                        }
                        if (property == "LccThreadD") {
                            var D_LccThread = jsonarray[0]["LccThreadD"];
                        }
                        if (property == "MultiThreadD") {
                            var D_MultiThread = jsonarray[0]["MultiThreadD"];
                        }
                        if (property == "SFCD") {
                            var D_SFCThread = jsonarray[0]["SFCD"];
                        }
                        if (property == "NewThreadD") {
                            var D_NewThread = jsonarray[0]["NewThreadD"];
                        }
                        if (property == "VIATHREAD") {
                            var Viathread = jsonarray[0]["VIATHREAD"];
                        }
                        if (property == "CORPORATETHREAD") {
                            var Corpthread = jsonarray[0]["CORPORATETHREAD"];
                        }
                        if (property == "RETAILTHREAD") {
                            var Retthread = jsonarray[0]["RETAILTHREAD"];
                        }
                    }

                    if (Thread != null && Thread != "") {
                        $("#hdthread").val(Thread);
                    }
                    if (FscThread != null && FscThread != "") {
                        $("#fscthreadkey").val(FscThread);
                    }
                    if (LccThread != null && LccThread != "") {
                        $("#lccthreadkey").val(LccThread);
                    }
                    if (MultiThread != null && MultiThread != "") {
                        $("#hdnMultiThread").val(MultiThread);
                    }
                    if (NewThread != null && NewThread != "") {
                        $("#newthreadkey").val(NewThread);
                    }
                    if (SFCThread != null && SFCThread != "") {
                        $("#hdnsfcthread").val(SFCThread);
                    }

                    if (D_threadkey != null && D_threadkey != "") {
                        $("#hdthread_dt").val(D_threadkey);
                    }
                    if (D_FscThread != null && D_FscThread != "") {
                        $("#fscthreadkey_dt").val(D_FscThread);
                    }
                    if (D_LccThread != null && D_LccThread != "") {
                        $("#lccthreadkey_dt").val(D_LccThread);
                    }
                    if (D_MultiThread != null && D_MultiThread != "") {
                        $("#hdnMultiThread_dt").val(D_MultiThread);
                    }
                    if (D_NewThread != null && D_NewThread != "") {
                        $("#newthreadkey_dt").val(D_NewThread);
                    }
                    if (Viathread != null && Viathread != "") {
                        $("#hdn_viathread").val(Viathread);
                    }


                }
            },
            error: function (e) {//On Successful service call
                if (e.status == "500") {
                    window.location.href = "@Url.Action("SessionExp", "Redirect")"
                    return false;
                }

            }	// When Service call fails

        });
    }
