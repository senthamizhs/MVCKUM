                    }

                }
            }
        }

        for (var fr = 0; fr < book_seatx.length; fr++) {
            if (book_seatx[fr].split('~')[3] != "" && book_seatx[fr].split('~')[3] != null && book_seatx[fr].split('~')[3] != "Not Selected") {
                seatamount += parseFloat(book_seatx[fr].split('~')[3]);
            }
        }
        //var par = $('#' + ptype).val().split('~');
        var tot = "";
        var totMeal = 0;
        var totBagg = 0;
        var GrossTot = 0;
        var grossbag = 0;
        try {
            var total_amount = $("#Li_Totalfare").data("li_totalfare");
            var new_seatamot = 0;
            var Seat_amount_row = 0;
            tot_seat_amount = seatamount;
            // var total_amount = document.getElementById("Li_Totalfare").innerHTML;
            var insamt = document.getElementById('insamount').innerHTML;
            GrossTotM = GrossTotM + totMeal;
            grossbagg = grossbagg + totBagg;
            var TotalGross = parseFloat(total_amount) + parseFloat(GrossTotM) + parseFloat(grossbagg) + parseFloat(tot_seat_amount) + parseFloat(insamt);
            document.getElementById("mealamount").innerHTML = GrossTotM;
            document.getElementById("BaggageAmnt").innerHTML = grossbagg;
            document.getElementById("Li_Totalfare").innerHTML = TotalGross;// parseFloat(total_amount);
            document.getElementById("seatamount").innerHTML = parseFloat(tot_seat_amount);

            if (AvailFormat == "NAT" || $("#hdn_HideComission").val() == "Y") {/* STS-166*/
                var totalfare = $("#totalAmnt").data("totalamntd");
                var TotalfareGross = parseFloat(totalfare) + parseFloat(GrossTotM) + parseFloat(grossbagg) + parseFloat(tot_seat_amount) + parseFloat(insamt);
                $("#totalAmnt").html(TotalfareGross);
            }

            document.getElementById("insamount").innerHTML = "0";

            if ($("#hdn_bookingprev").val() == "Y") {
                var seatamt = $(".riyasea")[0].innerHTML;
                $(".riyasea")[0].innerHTML = Number("0").toFixed(decimalflag);
                var totamt = $(".riyatotal")[0].innerHTML;
                $(".riyatotal")[0].innerHTML = Number(parseFloat(totamt) - parseFloat(seatamt)).toFixed(decimalflag)
            }

            $("span[id='spnFare']").text(TotalGross);

            var DDL = $('.ddlclass');
            $('.ddlclass').selectedIndex = 0
            //$('#' + ptype).val('');
            //ptype
        }
        catch (Error) {
            showerralert(Error.message, "", "");
            //alert(Error.message);
        }
    }
    function loadtop5transaction() {

        var Param = { ClentID: sessionStorage.getItem("clientid") };
        $.blockUI({
            message: '<img alt="Please Wait..." src="@strLoaderURL" style="background-color:#fff; border-radius:8px; margin-top:12%;" id="FareRuleLodingImage" />',
            css: { padding: '5px' }
        });
        $.ajax({
            type: "POST",//GET or POST or PUT or DELETE verb
            url: Last5Transaction,
            data: JSON.stringify(Param),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (json) {//On Successful service call
                $.unblockUI();
                if (json.Status == "01") {

                    var transactionresult = JSON.parse(json.Results).P_FETCH_LAST_TRANSACTIONS;
                    buildtables(transactionresult);
                }
                else {
                    if (json.Message != null && json.Message != "") {
                        showerralert(json.Message, "", "");
                        return false;
                    } else {
                        showerralert("Problem occure while fetching last transaction.Please contact customer care.-(#02)", "", "");
                        return false;
                    }

                }

            },
            error: function (e) {
                console.log(e.responseText);
                $.unblockUI();
            }
        });

    }
