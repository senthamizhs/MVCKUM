    function LoadCountryCode() {
        var CountryCodeURL = '@Url.Content("~/XML/CountryCodes.json?" + strFileversion)';
        $.ajax({
            type: "Get",
            url: CountryCodeURL,
            dataType: "json",
            success: function (data) {
                var codeBuilder = "";
                var strCountry = "";
                $.each(data, function (i, val) {
                    if ('@strCountry' == val.code)
                        codeBuilder += "<option val='" + val.dial_code + "' data-countrycode='" + val.code + "' selected>" + val.dial_code + "</option>"
                    else
                        codeBuilder += "<option val='" + val.dial_code + "' data-countrycode='" + val.code + "'>" + val.dial_code + "</option>"
                })
                $("#phncode").html(codeBuilder);
            },
            error: function () {
                $("#phncode").val("<option value='@strCountryCode'>@strCountryCode</option>");
            }
        });
    }
