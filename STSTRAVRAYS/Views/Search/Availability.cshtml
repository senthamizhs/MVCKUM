    function Confirmsms() {
        var MobileNumber = $("#txttomobnum").val();
        if (MobileNumber == "") {
            showerralert("Please Enter Mobile Number", 5000, "");
            return false;
        }
        else if (MobileNumber.length < 10) {
            showerralert("Please Enter valid  Mobile Number", 5000, "");
            return false;
        }
        var SMSFare = $("#edit_gross_sendsms").val();
        if (SMSFare.trim() == null || SMSFare.trim() == "") {
            showerralert("Please enter fare for send sms", 5000, "");
            return false;
        }
        var RequestData = {
            Origin: orgin,
            Destination: destination,
            DepartureDate: deptdate,
            FlightNo: flightno,
            GrossAmount: SMSFare,
            MobileNumber: MobileNumber
        };

        $.ajax({
            type: "POST",
            contentType: "application/json; charset=utf-8",
            url: "@Url.Action("SendAvailabilitySms", "Flights")",
            data: JSON.stringify(RequestData),
            dataType: "json",
            success: function (data) {
                $('#modal-Fare').iziModal('close');
                $("#txttomobnum").val("");
                $.unblockUI();
                if (data.status == "-1") {
                    window.location = sessionExb;
                }
                else if (data.status == "01") {
                    showerralert("SMS Sent Successfully", 5000, "");

                }
                else if (data.status == "00") {
                    showerralert(data.Error, 5000, "");

                }
                else {
                    showerralert(data.Error, 5000, "");
                }
            },
            error: function (e) {
                $('#modal-Fare').iziModal('close');
                $.unblockUI();
                showerralert('An Internal Problem Occurred. Your Session will Expire..', 5000, "");
            }
        });
    }

