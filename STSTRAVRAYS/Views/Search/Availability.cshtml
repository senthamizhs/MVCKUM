     string strProduct = System.Configuration.ConfigurationManager.AppSettings["Producttype"].ToString();
     string strAppTheme = System.Configuration.ConfigurationManager.AppSettings["apptheme"].ToString();
     string strAppName = System.Configuration.ConfigurationManager.AppSettings["Appname"].ToString().ToUpper();
     string MainCity = System.Configuration.ConfigurationManager.AppSettings["MainCity"].ToString();
     string ClientID = Session["POS_ID"] != null ? Session["POS_ID"].ToString() : "";
     string BranchID = Session["branchid"] != null ? Session["branchid"].ToString() : "";
     string ClientName = Session["agencyname"] != null ? Session["agencyname"].ToString() : "";
     string strTerminalType = Session["TERMINALTYPE"] != null && Session["TERMINALTYPE"].ToString() != "" ? Session["TERMINALTYPE"].ToString() : System.Configuration.ConfigurationManager.AppSettings["TerminalType"].ToString().ToUpper();
     string AllowCorptab = Session["ALLOW_CORP_FARE"] != null && Session["ALLOW_CORP_FARE"].ToString() != "" ? Session["ALLOW_CORP_FARE"].ToString() : "N";
     string SegmentFliterversion = ViewBag.fileversion != null && ViewBag.fileversion != "" ? ViewBag.fileversion : fileversion;
     string ShowCommission = (Session["ShowCommission"] != null && Session["ShowCommission"].ToString() != "") ? Session["ShowCommission"].ToString() : "1";
     string strDom1g = Session["ALLOW_DOM1G"] != null && Session["ALLOW_DOM1G"].ToString() != "" ? Session["ALLOW_DOM1G"].ToString() : "";
     string strInt1g = Session["ALLOW_INT1G"] != null && Session["ALLOW_INT1G"].ToString() != "" ? Session["ALLOW_INT1G"].ToString() : "";
     string strDom1s = Session["ALLOW_DOM1S"] != null && Session["ALLOW_DOM1S"].ToString() != "" ? Session["ALLOW_DOM1S"].ToString() : "";
     string strInt1s = Session["ALLOW_INT1S"] != null && Session["ALLOW_INT1S"].ToString() != "" ? Session["ALLOW_INT1S"].ToString() : "";
     string IsBSALogin = System.Configuration.ConfigurationManager.AppSettings["APP_HOSTING"].ToString().ToUpper();
     string strIsAgentlogedin = Session["IsAgentLogin"] != null && Session["IsAgentLogin"].ToString() != "" ? Session["IsAgentLogin"].ToString() : "N";
     string strShowEarnings = "display:none;";
     if (strIsAgentlogedin == "Y")
     {
         strShowEarnings = "display:block;";
     }
     var sessionstrg = Session["terminalid"] != null ? Session["terminalid"].ToString() : "";
     string strLoaderURL = System.Configuration.ConfigurationManager.AppSettings["LogoUrl"].ToString() + "/" + strProduct + "/" + "loader.gif?" + fileversion;
     string _mCityFlg = Session["multicity"] != null ? Session["multicity"].ToString().Trim() : "N";
     string _segFlg = Session["tripConfig"] != null ? Session["tripConfig"].ToString().Trim() : "O|Y"; // Oneway and Roundtrip
     string currenctdate_ad = Session["Serverdatetime"] != null ? Session["Serverdatetime"].ToString().Trim() : ViewBag.currentdate;
     string strOnlyInternational = System.Configuration.ConfigurationManager.AppSettings["ONLYINTERNATIONAL"].ToString();
     string Airporttypecss = "display:block;";
     if (strOnlyInternational == "Y")
     {
         Airporttypecss = "display:none;";
     }
     string HideComissionFor = System.Configuration.ConfigurationManager.AppSettings["MINUSCOMISSIONFOR"].ToString();
     string HideComission = "N";
     if (strIsAgentlogedin == "Y" && !string.IsNullOrEmpty(HideComissionFor) && (HideComissionFor.Contains("B2B") || HideComissionFor.Contains("BOA") || HideComissionFor == "ALL"))
     {
         HideComission = "Y";
     }
     else if (strIsAgentlogedin != "Y" && !string.IsNullOrEmpty(HideComissionFor) && (HideComissionFor.Contains("B2C") || HideComissionFor == "ALL"))
     {
         HideComission = "Y";
     }
     string strProcessingImage = Session["ProcessingImage"] != null && Session["ProcessingImage"].ToString() != "" ? Session["ProcessingImage"].ToString() + "?" + fileversion : strLoaderURL;
     string strSelectLoaderID =Session["ProcessingImage"] != null && Session["ProcessingImage"].ToString() != "" ? "AddLoaderImg" : "FareRuleLodingImage";
     string strDefaultAgent = Session["DEFAULT_AGENTID"] != null ? Session["DEFAULT_AGENTID"].ToString() : "";
}
