        function outstanding() {
            var OutDate = $("#txtoutstddate").val();
            var RData = [];
            var PData = [];
            var SData = [];

            $.blockUI({
                message: '<img alt="Please Wait..." src="@strLoaderURL" style="background-color:#fff; border-radius:8px; margin-top:12%;" id="FareRuleLodingImage" />',
                css: { padding: '5px' }
            });

            var Param = {
                strDate: OutDate,
                strAgentID: "",
                strTerminalID: "",
                strUserName: "",
                strAgentType: "",
                strSequenceId: "",
                strBranchID: "",
                strTerminalType: "",
                strIPAddress: "",

            }

            $.ajax({
                type: "POST",
                url: "@Url.Action("OutstandingReport", "Home")",
                data: JSON.stringify(Param),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (json) {

                    var result = json.Result;
                    if (json.Status == "00") {
                        showerralert(json.Message, "", "");
                    }
                    else if (json.Status == "01") {
                        if (result[1] != "" && result[2] != "") {
                            Data = JSON.parse(result[2]);
                            Data1 = JSON.parse(result[1]);
                            $('#hdfjsondata').val(JSON.stringify(Data1));
                            var NoOfRow = Data.length;
                            var str = "";
                            outamt = result[3] != null ? result[3] : "";
                            if (outamt != "") { $("#dvclrOtsAmt").show(); $("#dvclrOtsAmt").html("Outstanding Amount : <b> <i class='fa fa-rupee'></i> " + outamt + "</b>"); }

                            str = '<table id="tbl_outstd" data-paging="true" data-sorting="true" class="table table-striped footable footable-1 footable-filtering footable-filtering-right breakpoint-sm  footable tablet breakpoint footable-loaded">';
                            var row = 0;
                            for (var result in Data) {
                                var mainData = Data[result];
                                row++;
                                var i = 0;
                                var cls = "";
                                if (row == 1) {

                                    str += '<thead><tr class="footable-header" >';
                                    str += '<th >Topup Date</th>'
                                    str += '<th  data-breakpoints="xs" data-type="number">Delay Days</th>'
                                    str += '<th  data-breakpoints="xs" data-type="number">Topup Amount</th>'
                                    str += '<th  data-breakpoints="xs" data-type="number"><nobr>Receipt Amount</nobr></th>'
                                    str += '<th  data-type="number" >Outstanding Amount</th>'
                                   // str += '<th >Payment Due Date</th>'
                                    str += '<th  data-sort-ignore="true">All</th>'
                                    str += '</tr></thead><tbody>';
                                }
                                str += '<tr>';
                                for (key in mainData) {
                                    var dscdata = mainData["RefID"];
                                    if ((key == "Date") || (key == "Outstanding_Amount")) {

                                        if (key == "Outstanding_Amount") {
                                            str += '<td style="color:blue;cursor:pointer;text-align:right;text-decoration: underline;" id="clkoutstd_' + row + '" onclick=alltrackdetails(this.id) data-receiptid=' + dscdata + '>'

                                            str += '<span>' + mainData[key] + '</span>'
                                            str += '</td>'
                                        }
                                        else {
                                            str += '<td >' + mainData[key] + '</td>';
                                        }
                                    }
                                    else if (key == "PaymentDueDate") {
                                       // str += '<td class="footable-first-column" style="text-align:Right;">' + mainData[key] + '</td>';
                                    }
                                    else if (key == "RefID") {

                                        str += '<td><label class="label--checkbox"><input type="checkbox" class="checkbox" id="outstd_' + mainData["RefID"] + '" onclick=ClearOutstandingSelect(this.id) data-receiptid=' + mainData["RefID"] + ' data-outsdamt=' + mainData["Outstanding_Amount"] + '  /></label></td>'

