                    message: '<img alt="Please Wait..." src="@strLoaderURL" style="background-color:#fff; border-radius:8px; margin-top:12%;" id="FareRuleLodingImage" />',
                    css: { padding: '5px' }
                });

                $.ajax({
                    type: "POST",
                    url: "@Url.Action("ClEAROUTSTANDINGENCRYPTURL", "Home")",
                    contentType: "application/json; charset=utf-8",
                    data: '{strReceiptID: "' + strReceiptID + '",strReceiptAmt: "' + strReceiptAmt + '"}',
                    dataType: "json",
                    success: function (json) {
                        $.unblockUI();
                        var result = json.Result;
                        if (json.Status == "00") {
                            showerralert(json.Message, "", "");
                            return;

                        }
                        else if (json.Status == "01") {
                            if (result[1] != "" && result[1] != null) {
                                location.replace(result[1]);
                            }
                        }
                        else if (json.Status == "-1") {
                            SessionExpired(result[0])
                        }
                        else {
                            showerralert("Unable to connect to remote server! Please try again later!1", "", "");
                        }
                    },
                    error: function (result) {
                        console.log(result.statusText + "~" + result.status + "~" + result.responseText);
                        alert("Unable to connect to remote server! Please try again later!1");
                    }
                });
                return false;
            } catch (e) {
                console.log(e.message);
            }
        }

