        function Dateformat() {
            var currentDate = new Date();
            var day = currentDate.getDate()
            var month = currentDate.getMonth() + 1
            var year = currentDate.getFullYear()
            if (day < 10) { day = '0' + day } if (month < 10) { month = '0' + month }
            var getdates = day + "/" + month + "/" + year;
            var MinDate = day + "/" + month + "/" + (year - 1);
            $('#txtoutstddate').val(getdates);
            $("#txtoutstddate").addClass("has-content");
            $("#txtoutstddate").datepicker({
                maxDate: currentDate, dateFormat: "dd/mm/yy",
            });
