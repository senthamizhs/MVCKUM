        var secdata = [];
        var checkary = [];
        var popstr = "";

        function alltrackdetails(event) {
            RefID = $("#" + event).data("receiptid");
            var selectedRef = document.getElementById('hdfjsondata').value;
            secdata = JSON.parse(selectedRef);
            if (secdata.length > 0) {
                checkary = $.grep(secdata, function (n, i) {
                    return n["Ref ID"] == RefID;
                });
                if (checkary.length > 0) {
                    TableFormation(checkary);
                }
                else {
                    showpopup("<span style='color:white;'>OutStanding Statement</span>", "", "No Records", "", "", "");
                }
            }

        }

        var RecptID = ""; var RecptAmt = "0";
        function ClearOutstandingSelect(event) {
            var ReceiptID = $("#" + event).data("receiptid");
            var ReceiptAMT = $("#" + event).data("outsdamt");

            if ($("#outstd_" + ReceiptID)[0].checked == true) {
                RecptID += ReceiptID + "|"; RecptAmt = parseFloat(parseInt(RecptAmt) + parseInt(ReceiptAMT));
            }
            else {
                if (RecptID != 0) {
                    RecptID = RecptID.replace(ReceiptID + "|", "");
                    RecptAmt = parseFloat(parseInt(RecptAmt) - parseInt(ReceiptAMT));
                }
            }
            $("#hdnReceiptID").val(RecptID); $("#dvOtsAmt").html(RecptAmt);
            $("#hdnReceiptID").val() == "" ? $("#dvclearoutstand").hide() : $("#dvclearoutstand").hide()

        }

        function btnbtnClearoutstand() {
            strReceiptID = $("#hdnReceiptID").val();
            strReceiptAmt = RecptAmt;
            try {
                if ($("#hdnReceiptID").val() == "") {
                    alert("Please select any one Clearoutstanding Amount.");
                    return false;
                }

